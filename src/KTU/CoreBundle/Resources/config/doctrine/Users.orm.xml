<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="KTU\CoreBundle\Entity\Users" table="users">
    <id name="idusers" type="integer" column="idusers">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="15" nullable="true"/>
    <field name="password" type="string" column="password" length="45" nullable="true"/>
    <field name="email" type="string" column="email" length="45" nullable="true"/>
    <field name="age" type="string" column="age" length="45" nullable="true"/>
    <field name="gender" type="string" column="gender" length="5" nullable="true"/>
    <field name="regDate" type="date" column="reg_date" nullable="true"/>
  </entity>
</doctrine-mapping>
