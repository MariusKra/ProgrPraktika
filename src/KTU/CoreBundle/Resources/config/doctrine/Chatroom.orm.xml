<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="KTU\CoreBundle\Entity\Chatroom" table="chatroom">
    <indexes>
      <index name="fk_chatroom_chatroom_partisipants_idx" columns="chatroom_participants_id"/>
    </indexes>
    <id name="idchatroom" type="integer" column="idchatroom">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true"/>
    <many-to-one field="chatroomParticipants" target-entity="ChatroomPartisipants">
      <join-columns>
        <join-column name="chatroom_participants_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
