<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="KTU\CoreBundle\Entity\Darbuotojai" table="Darbuotojai">
    <indexes>
      <index name="ViesbutisID" columns="ViesbutisID"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="ID_UNIQUE" columns="ID"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="vardas" type="string" column="Vardas" length="20" nullable="true"/>
    <field name="pavarde" type="string" column="Pavarde" length="20" nullable="true"/>
    <field name="pareigos" type="string" column="Pareigos" length="20" nullable="true"/>
    <field name="dirbaNuo" type="date" column="Dirba_nuo" nullable="true"/>
    <field name="atlyginimas" type="integer" column="Atlyginimas" nullable="true"/>
    <many-to-one field="viesbutisid" target-entity="Viesbuciai">
      <join-columns>
        <join-column name="ViesbutisID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
